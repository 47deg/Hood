    maven { url = uri("https://dl.bintray.com/47deg/helios") }
}

dependencies {
    compile "com.47deg:helios-core:0.2.0"
    compile "com.47deg:helios-parser:0.2.0"
    compile "com.47deg:helios-optics:0.2.0"
